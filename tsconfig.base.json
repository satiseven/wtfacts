{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["development"],
    "paths": {
      "@wtfacts/auth": ["libs/feature/auth/src/index.ts"],
      "@wtfacts/auth-utils": ["libs/auth/utils/src/index.ts"],
      "@wtfacts/category-entity": [
        "libs/database/entities/category/src/index.ts"
      ],
      "@wtfacts/client": ["libs/api/client/src/index.ts"],
      "@wtfacts/components": ["libs/ui/components/src/index.ts"],
      "@wtfacts/config": ["libs/common/config/src/index.ts"],
      "@wtfacts/dto": ["libs/api/dto/src/index.ts"],
      "@wtfacts/entities": ["libs/database/entities/src/index.ts"],
      "@wtfacts/fact": ["libs/feature/fact/src/index.ts"],
      "@wtfacts/fact-entity": ["libs/database/entities/fact/src/index.ts"],
      "@wtfacts/guards": ["libs/auth/guards/src/index.ts"],
      "@wtfacts/hooks": ["libs/ui/hooks/src/index.ts"],
      "@wtfacts/icons": ["libs/ui/icons/src/index.ts"],
      "@wtfacts/interfaces": ["libs/api/interfaces/src/index.ts"],
      "@wtfacts/logger": ["libs/common/logger/src/index.ts"],
      "@wtfacts/migrations": ["libs/database/migrations/src/index.ts"],
      "@wtfacts/models": ["libs/api/models/src/index.ts"],
      "@wtfacts/repositories": ["libs/database/repositories/src/index.ts"],
      "@wtfacts/search": ["libs/feature/search/src/index.ts"],
      "@wtfacts/seeds": ["libs/database/seeds/src/index.ts"],
      "@wtfacts/strategies": ["libs/auth/strategies/src/index.ts"],
      "@wtfacts/styles": ["libs/ui/styles/src/index.ts"],
      "@wtfacts/tag-entity": ["libs/database/entities/tag/src/index.ts"],
      "@wtfacts/user": ["libs/feature/user/src/index.ts"],
      "@wtfacts/user-entity": ["libs/database/entities/user/src/index.ts"],
      "@wtfacts/utils": ["libs/common/utils/src/index.ts"]
    }
  }
}
